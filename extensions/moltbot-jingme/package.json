{
  "name": "@jd-moltbot/jingme",
  "version": "0.0.2",
  "type": "module",
  "description": "Clawdbot JingMe (äº¬me) channel plugin with enhanced features, improved error handling, and security",
  "clawdbot": {
    "extensions": [
      "./dist/index.js"
    ]
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true})\"",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "registry": "http://registry.m.jd.com/",
    "access": "restricted"
  },
  "keywords": [
    "clawdbot",
    "jingme",
    "messaging",
    "wechat",
    "jd.com",
    "channel",
    "plugin"
  ],
  "author": "Plugin Developer",
  "license": "MIT",
  "dependencies": {
    "undici": "^6.0.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^18.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md"
  ]
}
